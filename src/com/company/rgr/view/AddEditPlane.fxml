                  <Label prefHeight="27.0" prefWidth="130.0" text="Макс. вес:" />
                  <TextField fx:id="etCarryingCapacity" />
               </children></HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label prefHeight="27.0" prefWidth="130.0" text="Дальность полета:" />
                  <TextField fx:id="etRangeOfFlight" />
               </children></HBox>
         </children>
      </VBox>
      <Button fx:id="apply" layoutX="14.0" layoutY="311.0" mnemonicParsing="false" onMouseClicked="#onClick" prefHeight="26.0" prefWidth="128.0" text="Button" />
   </children>
</AnchorPane>
